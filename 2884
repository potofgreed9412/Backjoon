#define _CRT_SECURE_NO_WARNINGS
#include <stdio.h>
int main()
{
  int a,b;
  scanf("%d %d",&a,&b);

if(a>=1 && a<=23 && b>=45)
  printf("%d %d", a,b-45);

else if(a>=1 && a<=23 && b<45)
  printf("%d %d", a-1,b+15);

if(a==0 && b>=45)
  printf("%d %d", a,b-45);

else if(a==0 && b<45)
  printf("%d %d", 23,b+15);
  
  system("PAUSE");
}
